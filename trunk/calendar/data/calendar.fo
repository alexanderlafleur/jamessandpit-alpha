				          <fo:table-cell border-width="1pt" border-color="black" border-style="solid" padding="1mm"><fo:block>30</fo:block></fo:table-cell>
				          <fo:table-cell border-width="1pt" border-color="black" border-style="solid" padding="1mm"><fo:block>31</fo:block></fo:table-cell>
